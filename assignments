 JAVA SCRIPT CLASS ->
 class Human{
    constructor(name,age,FatherName){
        this.name=name;
        this.age=age;
        this.FatherName=FatherName;
    }
        getFullName(){
        
            return this.name+" S/o "+this .FatherName;
        
    }
}

let MyHuman = new Human("Chaman",19,"Laxman Singh");

console.log(MyHuman.getFullName());
--------------------------------------------------------------------
class Human{
    constructor(name,year){
        this.name=name;
        this.year=year;
    }
    age(){
         let date=new Date();
       return date.getFullYear() - this.year;
    }
}

var myHuman=new Human("Chaman",2002);
console.log(myHuman.age());
-----------------------------------------------------------------------

class student{
    constructor(Name,Clas,RollNo,Age){
        this.Name=Name;
        this.Clas=Clas;
        this.RollNo=RollNo;
        this.Age=Age;
    }

    getStudentInfo() {
        return "Student Name is "+this.Name+". And Class is "+this.Clas+". And Roll Number is "+this.RollNo+"And Student Age is "+this.Age;
    }
}

var IsStudent=new student("Chaman Rawat","BCA First Year",10012,19);
console.log(IsStudent.getStudentInfo());
 
 
